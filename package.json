    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.10",
    "lint-staged": "13.0.2",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.3.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "ts-node": "^10.9.2"
  }
}
